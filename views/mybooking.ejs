        <h3 class="heading">My Booked Appointmemt</h3>
        <table class="table" style="margin-top: 40px; text-align: center;">
            <thead class="table-dark">
              <th>Name</th>
              <th>Ph NO.</th>
              <th>Email</th>
              <th>Doctor</th>
              <th>Date</th>
              <th>Time</th>
              <th>Remarks</th>
            </thead>
            <tbody>
                <td><%=data.name%></td>
                <td><%=data.ph%></td>
                <td><%=data.email%></td>
                <td><%=data.doctor%></td>
                <td><%=data.date%></td>
                <td><%=data.time%></td>
                <td><%=data.remarks%></td>
            </tbody>
            
          </table>
        
    </div>
